@startuml

class User {
    +uid: String
    +email: String
}

class GroupMember {
    +id: Long
    +user: User
    +group: StudyGroup
}

class StudyGroup {
    +id: Long
    +name: String
    +imageUrl: String
    +joinCode: String
    +members: List<GroupMember>
}

class FocusSession {
    +id: String
    +user: User
    +session: Session
    +course: Course
    +topic: Topic
    +assignment: Assignment
    +focusStart: LocalDateTime
    +focusEnd: LocalDateTime
    +elapsedSeconds: Long
    +plannedDuration: Long
    +status: FocusStatus
    +sessionType: SessionType
}

@enduml